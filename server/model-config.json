{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "ApiUser": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "findById": true,
          "login": true,
          "logout": true,
          "isInRole": true,
          "findUserIdByToken": true,
          "prototype.__destroyById__accessTokens": true,
          "prototype.__findById__accessTokens": true,
          "prototype.__get__accessTokens": true
        }
      }
    }
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "TranscriptionControl": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "findOwnedById": true,
          "transcribeAudio": true,
          "getAudioUrl": true,
          "cleanupExistingResources": true
        }
      }
    }
  }
}
